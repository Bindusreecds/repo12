..................call by referncre.........................

#include <stdio.h>

void swap(int* a,int* b)
{
    int temp;
    temp=*a;
    *a=*b;
    *b=temp;
    printf("after swaping \t %d,\t %d",*a,*b);
}
void main()
{
    int a=10,b=20;
    printf("before swaping \t %d,\t %d",a,b);
    swap(&a,&b);
    printf("after swaping \t %d,\t%d",a,b);
}
.................call by value..........
#include <stdio.h>

void swap(int a,int b)
{
    int temp;
    temp=a;
    a=b;
    b=temp;
    printf("after swaping \t %d,\t %d",a,b);
}
void main()
{
    int a=10,b=20;
    printf("before swaping \t %d,\t %d",a,b);
    swap(a,b);
    printf("after swaping \t %d,\t%d",a,b);
}


.......................big number..................
#include<stdio.h>
int main()
{
    int i,number=0,big=0;
    printf("enter 10 number");
    for(i=0;i<=10;i++)
    {
        scanf("%d",&number);
        if(number>big)
        big=number;
    }
    printf("biggest number is :%d",big);
}

.................bid number using malloc....................
#include<stdio.h>
int main()
{
    int *a=(int*)malloc(10*sizeof(int));
    int big=0;
    printf("enter the number");
    for(int i=0;i<=10;i++)
    {
        scanf("%d",&(*(a+i)));
        if(big >(*(a+i)));
        {
        big=*(a+i);
    }
    }
    printf("biggest number is :%d",big);
    return 0;
    }